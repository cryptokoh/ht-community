{
  "name": "healing-temple-community",
  "version": "1.0.0",
  "description": "The Healing Temple Community App - iOS and Android app for member check-ins, community features, and store credit tracking",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "expo": "54.0.0-canary-20250722-599a28f",
    "expo-barcode-scanner": "~12.5.3",
    "expo-camera": "16.1.12-canary-20250722-599a28f",
    "expo-dev-client": "^5.1.9-canary-20250722-599a28f",
    "expo-image-picker": "~16.1.4",
